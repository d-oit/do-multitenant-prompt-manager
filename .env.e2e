RATE_LIMIT_MAX_REQUESTS=1000
RATE_LIMIT_WINDOW_SECONDS=60
E2E_TEST_MODE=true

# Frontend configuration for e2e tests
VITE_API_BASE_URL=http://localhost:8787
VITE_LOG_LEVEL=debug
