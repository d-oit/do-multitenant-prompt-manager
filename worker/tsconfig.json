{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "ignoreDeprecations": "6.0",
    "strict": true,
    "noEmit": true,
    "types": ["@cloudflare/workers-types"],
    "lib": ["ES2022"],
    "skipLibCheck": true,
    "paths": {
      "../../shared/*": ["../shared/*"]
    }
  },
  "include": ["src/**/*.ts", "test/**/*.ts", "../shared/**/*.ts"]
}
